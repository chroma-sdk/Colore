{
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": [
            "Colore/bin/Release/netstandard2.1/Colore.dll"
          ],
          "exclude": [ "**/obj/**" ]
        }
      ],
      "dest": "api",
      "properties": { "TargetFramework": "netstandard2.1" },
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      },
      {
        "src": "..",
        "files": [
          "*.md",
          "LICENSE",
          "AUTHORS",
          "CODEOWNERS"
        ],
        "dest": "./"
      }
    ],
    "resource": [
      {
        "files": [ "images/**" ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [ "apidoc/**.md" ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ],
    "dest": "_site",
    "globalMetadata": {
      "_appLogoPath": "images/colore.png",
      "_enableSearch": true,
      "_appFooter": "Copyright Â© 2015-2022 by <a href=\"https://github.com/Sharparam\">Adam Hellberg</a> and <a href=\"https://github.com/brandonscott\">Brandon Scott</a><br>Generated by <a href=\"http://dotnet.github.io/docfx/\"><strong>DocFX</strong></a>"
    },
    "fileMetadataFiles": [],
    "template": [ "default", "theme" ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "sitemap": {
      "baseUrl": "https://chroma-sdk.github.io/Colore/docs/"
    }
  }
}
